{
  "name": "crypto-bot",
  "version": "1.0.0",
  "description": " Telegram bot for cryptocurrency prices",
  "scripts": {
    "start": "cross-env NODE_ENV=\"prod.env\" ts-node ./src/index.ts",
    "start:dev": "cross-env NODE_ENV=\"dev.env\" nodemon ./src/index.ts",
    "update": "cross-env NODE_ENV=\"dev.env\" ts-node ./src/update.ts",
    "format": "prettier --write \"src/**/*.ts\" \"src/*.ts\"",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "keywords": [
    "Telegram",
    "bot",
    "bot",
    "cryptocurrency",
    "crypto"
  ],
  "author": "Ziya",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^28.1.8",
    "@types/node": "^16.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^28.1.3",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "source-map-support": "^0.5.20",
    "ts-jest": "^28.0.8"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.1",
    "redis": "^4.6.5",
    "redis-om": "^0.3.6",
    "telegraf": "^4.8.6",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.0.0",
    "tslog": "^3.3.4",
    "typegram": "^3.10.0",
    "typescript": "^4.3.5"
  }
}
